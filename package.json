{
  "name": "mcp-odos",
  "version": "0.0.1",
  "description": "Mcp server for Odos access",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "pnpm run build",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "packageManager": "pnpm@10.7.1",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.js"
    },
    "./tool-selector": {
      "import": "./src/tool-selector.ts",
      "require": "./src/tool-selector.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IQAIcom/mcp-odos.git"
  },
  "keywords": [
    "mcp",
    "Odos",
    "iqagents"
  ],
  "author": "IQAI",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IQAIcom/mcp-odos/issues"
  },
  "homepage": "https://github.com/IQAIcom/mcp-odos#readme",
  "dependencies": {
    "@types/node": "^22.15.19",
    "@biomejs/biome": "*",
    "dedent": "^1.6.0",
    "express": "^5.1.0",
    "fastmcp": "^1.27.7",
    "viem": "^2.30.0",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.19",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
